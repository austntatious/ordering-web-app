$('.js-line-item-<%= @line_item.id %>').replaceWith('<%=j render @line_item %>');
var minus = 0;
if ($('.js-money-account').length) {
  minus = parseFloat($('.js-money-account').val());
}
$('.js-cart-price').html('$' + (<%= @current_cart.total_price_with_coupon %> - minus));
$('.js-cart-price').data('price', <%= @current_cart.total_price_with_coupon %>);
$('.js-cart-button').replaceWith('<%=j render @current_cart %>');
$('.js-tax-price').html('<%= number_to_currency @current_cart.tax_price %>');
$('.js-items-price').html('<%= number_to_currency @current_cart.products_price %>');
